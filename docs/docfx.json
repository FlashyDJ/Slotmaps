{
  "metadata": [
    {
      "src": [
        {
          "src": "../src",
          "files": "**.csproj",
          "exclude": [ "**/bin/**", "**/obj/**", "**/[Tt]ests/**" ]
        }
      ],
      "dest": "api",
      "outputFormat": "mref",
      "includePrivateMembers": false,
      "disableGitFeatures": false,
      "disableDefaultFilter": false,
      "noRestore": false,
      "namespaceLayout": "nested",
      "memberLayout": "separatePages",
      "EnumSortOrder": "declaringOrder",
      "allowCompilationErrors": false
    }
  ],
  "build": {
    "content": [
      { "files": ["api/**.yml"] },
      { "files": ["docs/**.md", "docs/**/toc.yml", "toc.yml", "*.md"] },
      { "files": ["samples/**.md", "samples/**.yml"] }
    ],
    "resource": [
      {
        "files": ["images/**"]
      }
    ],
    "overwrite": "overwrite/**.md",
    "markdownEngineName": "markdig",
    "markdownEngineProperties": {
      "alerts": {
        "INFO": "alert alert-info"
      }
    },
    "output": "_site",
    "globalMetadata": {
      "_appTitle": "Slotmaps",
      "_appName": "Slotmaps",
      "_appFooter": "<span>Copyright 2023 <a href=\"https://github.com/FlashyDJ\">FlashyDJ</a>. Made with <a href=\"https://dotnet.github.io/docfx\">DocFX</a>.</span>",
      "_enableSearch": true,
      "_enableNewTab": true,
      "_gitUrlPattern": "github",
      "_gitContribute": {
        "repo": "https://github.com/FlashyDJ/Slotmaps",
        "branch": "main"
      }
    },
    "sitemap": {
      "baseUrl": "https://FlashyDJ.github.io/Slotmaps",
      "priority": 0.5,
      "changeFreq": "monthly"
    },
    "globalMetadataFiles": [],
    "fileMetadataFiles": [],
    "template": [
      "default",
      "modern"
    ],
    "postProcessors": [],
    "keepFileLink": false,
    "disableGitFeatures": false
  }
}