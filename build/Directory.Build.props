<Project>

  <PropertyGroup>
    <TargetFramework>net8.0</TargetFramework>
    <LangVersion>preview</LangVersion>
    <Nullable>enable</Nullable>
  </PropertyGroup>

  <PropertyGroup>
    <PackageId>FlashyDJ.$(MSBuildProjectName)</PackageId>
    <Title>FlashyDJ.$(MSBuildProjectName)</Title>
    <RepositoryUrl>https://github.com/FlashyDJ/Slotmaps/</RepositoryUrl>
    <RepositoryType>git</RepositoryType>
    <PublishRepositoryUrl>true</PublishRepositoryUrl>
    <ProjectUrl>https://flashydj.github.io/Slotmaps/</ProjectUrl>
    <Authors>FlashyDJ</Authors>
    <Owners>FlashyDJ</Owners>
    <Company>FlashyDJ</Company>
    <Copyright>Copyright (c) 2023 FlashyDJ</Copyright>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <PackageProjectUrl>https://github.com/FlashyDJ/Slotmaps/</PackageProjectUrl>
    <PackageTags>datastructure;slotmap;collections</PackageTags>
  </PropertyGroup>

  <ItemGroup>
    <None Include="..\..\README.md" Pack="true" PackagePath=""/>
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="DotNet.ReproducibleBuilds" Version="1.1.1" PrivateAssets="all" />
  </ItemGroup>

</Project>